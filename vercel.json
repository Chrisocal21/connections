{
  "version": 2,
  "buildCommand": "node server.js",
  "rewrites": [
    {
      "source": "/connections",
      "destination": "/games/connections.html"
    },
    {
      "source": "/games/connections",
      "destination": "/connections"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "runtime": "@vercel/node@2.15.8",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}