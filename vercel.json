{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/connections",
      "dest": "/games/connections.html"
    },
    {
      "src": "/games/connections",
      "dest": "/connections"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}