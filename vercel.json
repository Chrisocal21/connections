{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/connections",
      "dest": "/games/connections.html"
    },
    {
      "src": "/games/connections",
      "dest": "/connections"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}